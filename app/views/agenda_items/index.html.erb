<h1>Listing agenda_items</h1>

<table>
  <thead>
    <tr>
      <th>Ends at</th>
      <th>Starts at</th>
      <th>Completed</th>
      <th>Trashed</th>
      <th>Description</th>
      <th>Position</th>
      <th>Presenter</th>
      <th>Name</th>
      <th>Meeting</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agenda_items.each do |agenda_item| %>
      <tr>
        <td><%= agenda_item.ends_at %></td>
        <td><%= agenda_item.starts_at %></td>
        <td><%= agenda_item.completed %></td>
        <td><%= agenda_item.trashed %></td>
        <td><%= agenda_item.description %></td>
        <td><%= agenda_item.position %></td>
        <td><%= agenda_item.presenter %></td>
        <td><%= agenda_item.name %></td>
        <td><%= agenda_item.meeting_id %></td>
        <td><%= link_to 'Show', agenda_item %></td>
        <td><%= link_to 'Edit', edit_agenda_item_path(agenda_item) %></td>
        <td><%= link_to 'Destroy', agenda_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agenda item', new_agenda_item_path %>
